<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/Silver"
    android:orientation="vertical"
    android:padding="@dimen/Margins"
    tools:context=".M020204">

    <TextView
        android:id="@+id/m020204_t000"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal|center_vertical"
        android:text="@string/m020204_t000"
        android:textColor="@color/black"
        android:textSize="@dimen/fontsize"
        android:textStyle="bold" />

    <EditText
        android:id="@+id/m020204_e001"
        android:layout_width="match_parent"
        android:layout_height="@dimen/p50"
        android:layout_marginTop="@dimen/Margins"
        android:layout_marginEnd="@dimen/p20"
        android:layout_marginBottom="@dimen/p20"
        android:background="@color/White"
        android:hint="@string/m020204_e001"
        android:inputType="textAutoCorrect"
        android:textSize="@dimen/fontsize" />

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/m020204_t001"
            android:layout_width="@dimen/p185"
            android:layout_height="@dimen/p100"
            android:layout_marginBottom="@dimen/Margins"
            android:background="@color/purple_200"
            android:gravity="center"
            android:text="@string/m020204_t001"
            android:textSize="@dimen/fontsize"
            android:visibility="visible"
            tools:ignore="TextContrastCheck" />

        <Button
            android:id="@+id/m020204_b001"
            android:layout_width="@dimen/p50"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/Spacing"
            android:text="@string/m020204_add"
            android:textSize="@dimen/fontsize"
            tools:ignore="DuplicateSpeakableTextCheck" />

        <Button
            android:id="@+id/m020204_b002"
            android:layout_width="@dimen/p50"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/Margins"
            android:text="@string/m020204_delete"
            android:textSize="@dimen/fontsize"
            tools:ignore="TextContrastCheck,DuplicateSpeakableTextCheck" />

        <Button
            android:id="@+id/m020204_b003"
            android:layout_width="@dimen/p50"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/Margins"
            android:text="@string/m020204_thumb"
            android:textAllCaps="false"
            android:textSize="@dimen/fontsize"
            tools:ignore="DuplicateSpeakableTextCheck" />
    </TableRow>

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/m020204_t002"
            android:layout_width="@dimen/p185"
            android:layout_height="@dimen/p100"
            android:layout_marginBottom="@dimen/Margins"
            android:background="@color/purple_200"
            android:gravity="center"
            android:text="@string/m020204_t002"
            android:textSize="@dimen/fontsize"
            android:visibility="visible"
            tools:ignore="TextContrastCheck" />

        <Button
            android:id="@+id/m020204_b004"
            android:layout_width="@dimen/p50"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/Spacing"
            android:text="@string/m020204_add"
            android:textSize="@dimen/fontsize" />

        <Button
            android:id="@+id/m020204_b005"
            android:layout_width="@dimen/p50"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/Margins"
            android:text="@string/m020204_delete"
            android:textSize="@dimen/fontsize"
            tools:ignore="TextContrastCheck" />

        <Button
            android:id="@+id/m020204_b006"
            android:layout_width="@dimen/p50"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/Margins"
            android:text="@string/m020204_thumb"
            android:textAllCaps="false"
            android:textSize="@dimen/fontsize" />
    </TableRow>


    <TableRow
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <Button
            android:id="@+id/m020204_b007"
            android:layout_width="@dimen/p185"
            android:layout_height="@dimen/p100"
            android:layout_marginTop="@dimen/p20"
            android:layout_marginEnd="@dimen/p20"
            android:background="@android:color/holo_blue_bright"
            android:gravity="center"
            android:text="@string/m020204_b007"
            android:textSize="@dimen/fontsize"
            android:visibility="visible" />

        <Button
            android:id="@+id/m020204_b008"
            android:layout_width="@dimen/p185"
            android:layout_height="@dimen/p100"
            android:layout_marginTop="@dimen/p20"
            android:layout_marginEnd="@dimen/Margins"
            android:background="@android:color/holo_blue_bright"
            android:gravity="center"
            android:text="@string/m020204_b008"
            android:textSize="@dimen/fontsize"
            android:visibility="visible" />
    </TableRow>

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <Button
            android:id="@+id/m020204_b009"
            android:layout_width="@dimen/p185"
            android:layout_height="@dimen/p100"
            android:layout_marginTop="@dimen/p20"
            android:layout_marginEnd="@dimen/p20"
            android:background="@android:color/holo_blue_bright"
            android:gravity="center"
            android:text="@string/m020204_b009"
            android:textSize="@dimen/fontsize"
            android:visibility="visible" />

        <Button
            android:id="@+id/m020204_b010"
            android:layout_width="@dimen/p185"
            android:layout_height="@dimen/p100"
            android:layout_marginTop="@dimen/p20"
            android:layout_marginEnd="@dimen/Margins"
            android:background="@android:color/holo_blue_bright"
            android:gravity="center"
            android:text="@string/m020204_b010"
            android:textSize="@dimen/fontsize"
            android:visibility="visible" />

    </TableRow>

</LinearLayout>