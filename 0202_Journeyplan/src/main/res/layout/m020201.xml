<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/Silver"
    android:orientation="vertical"
    android:padding="@dimen/Margins"
    tools:context=".M020201">

    <TextView
        android:id="@+id/m020202_t000"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal|center_vertical"
        android:text="@string/m020201_t000"
        android:textColor="@color/black"
        android:textSize="@dimen/fontsize"
        android:textStyle="bold" />

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/Margins">

        <TextView
            android:id="@+id/m020202_t001"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/p20"
            android:background="@color/Silver"
            android:text="@string/m020201_t001"
            android:textSize="@dimen/fontsize" />

        <TextView
            android:id="@+id/m020201_t002"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/p20"
            android:background="@color/Silver"
            android:text="@string/m020201_t002"
            android:textSize="@dimen/fontsize" />

        <TextView
            android:id="@+id/m020201_t003"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/p20"
            android:background="@color/Silver"
            android:text="@string/m020201_t003"
            android:textSize="@dimen/fontsize" />

        <Button
            android:id="@+id/m020202_b002"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/m020201_b001"
            android:textSize="@dimen/fontsize" />


    </TableRow>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/m020202_b001"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginTop="10dp"
            android:text="@string/m020201_t004_1"
            android:textSize="@dimen/fontsize" />

        <TextView
            android:id="@+id/m020202_b003"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/m020202_b001"
            android:layout_alignParentStart="true"
            android:layout_marginTop="6dp"
            android:text="@string/m020201_t004_2"
            android:textSize="@dimen/fontsize" />

        <TextView
            android:id="@+id/m020202_t003"
            android:layout_width="@dimen/p270"
            android:layout_height="@dimen/p50"
            android:layout_alignTop="@+id/m020202_b001"
            android:layout_marginStart="35dp"
            android:layout_marginTop="14dp"
            android:layout_toEndOf="@+id/m020202_b001"
            android:background="@color/teal_200"
            android:gravity="center_vertical"
            android:text="@string/m020201_t004"
            android:textSize="@dimen/fontsize"
            android:visibility="visible" />

        <TextView
            android:id="@+id/m020202_b006"
            android:layout_width="@dimen/p270"
            android:layout_height="wrap_content"
            android:layout_below="@+id/m020202_t003"
            android:layout_alignStart="@+id/m020202_t003"
            android:layout_marginTop="40dp"
            android:background="@color/purple_200"
            android:gravity="center_vertical"
            android:text="@string/m020201_t006"
            android:textSize="@dimen/fontsize"
            android:visibility="visible"
            tools:ignore="TextContrastCheck" />

        <TextView
            android:id="@+id/m020202_b004"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/m020202_b003"
            android:layout_alignParentStart="true"
            android:layout_marginTop="95dp"
            android:text="@string/m020201_t005_1"
            android:textSize="@dimen/fontsize" />

        <TextView
            android:id="@+id/m020202_b005"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/m020202_b004"
            android:layout_alignParentStart="true"
            android:layout_marginTop="6dp"
            android:text="@string/m020201_t005_2"
            android:textSize="@dimen/fontsize" />

        <TextView
            android:id="@+id/m020202_t004"
            android:layout_width="@dimen/p270"
            android:layout_height="@dimen/p50"
            android:layout_alignTop="@+id/m020202_b004"
            android:layout_marginStart="35dp"
            android:layout_marginTop="14dp"
            android:layout_toEndOf="@+id/m020202_b004"
            android:background="@color/teal_200"
            android:gravity="center_vertical"
            android:text="@string/m020201_t005"
            android:textSize="@dimen/fontsize" />

    </RelativeLayout>

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/p185">

        <Button
            android:id="@+id/m020201_b002"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="15dp"
            android:layout_marginEnd="@dimen/end"
            android:text="@string/m020201_b002"
            android:textSize="@dimen/font18" />

        <Button
            android:id="@+id/m020201_b003"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/end"
            android:text="@string/m020201_b003"
            android:textSize="@dimen/font18" />

        <Button
            android:id="@+id/m020201_b004"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/p10"
            android:text="@string/m020201_b004"
            android:textSize="@dimen/font18" />
    </TableRow>

</LinearLayout>